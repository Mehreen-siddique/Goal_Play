# Firebase Configuration - Environment Variables
# Copy this file to .env and fill in your actual API keys
# NEVER commit the actual .env file to version control!

# Firebase Project Configuration
FIREBASE_PROJECT_ID=goal-play
FIREBASE_AUTH_DOMAIN=goal-play.firebaseapp.com
FIREBASE_STORAGE_BUCKET=goal-play.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=480773029928

# API Keys (REPLACE WITH NEW KEYS AFTER REVOKING OLD ONES)
FIREBASE_WEB_API_KEY=your_new_web_api_key_here
FIREBASE_ANDROID_API_KEY=your_new_android_api_key_here
FIREBASE_IOS_API_KEY=your_new_ios_api_key_here

# App IDs
FIREBASE_WEB_APP_ID=your_new_web_app_id_here
FIREBASE_ANDROID_APP_ID=your_new_android_app_id_here
FIREBASE_IOS_APP_ID=your_new_ios_app_id_here
FIREBASE_WINDOWS_APP_ID=your_new_windows_app_id_here

# iOS Bundle ID
FIREBASE_IOS_BUNDLE_ID=com.example.goalPlay
